{
    "name": "babel-plugin-inject-react-style",
    "version": "0.0.1",
    "description": "A babel plugin for inject css style into react jsx element!",
    "types": "dist/index.d.ts",
    "main": "dist/index.js",
    "files": [
        "src",
        "dist"
    ],
    "scripts": {
        "start": "esc ./src",
        "build": "yarn clean && yarn compile",
        "watch": "yarn compile --watch",
        "compile": "esc -p ./src",
        "prepublishOnly": "yarn build",
        "test": "jest",
        "clean": "rm -rf ./dist"
    },
    "lazy": true,
    "sideEffects": false,
    "repository": "git@github.com:edonet/babel-plugin-inject-react-style.git",
    "author": "edonet@163.com",
    "license": "MIT",
    "dependencies": {
        "@babel/core": "^7.17.5",
        "@babel/helper-plugin-utils": "^7.16.7",
        "@types/jest": "^27.4.1"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@types/babel__core": "^7.1.18",
        "@types/babel__helper-plugin-utils": "^7.10.0",
        "@types/node": "^17.0.21",
        "@types/react": "^17.0.40"
    }
}
